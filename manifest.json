{
  "manifest_version": 2,
  "name": "baumanki.da",
  "description": "Download in 1 click without waiting for 30 seconds.",
  "version": "1.0",
  
   "browser_action": {
   "default_title": "baumanki.da",
   "default_icon": "icon.png",
   "default_popup": "popup.html"	
  },
  "permissions": [
   "http://*/*","https://*/*"
   ],
   "content_scripts": [
  {
    "matches": [
      "http://*/*","https://*/*"
    ],
    "js": ["content_script.js"]
  }
]
}